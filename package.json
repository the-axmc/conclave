  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:server": "tsx server/index.ts",
    "dev:mcp": "tsx mcp/conclave-tools/index.ts",
    "dev": "vite",
    "dev:full": "concurrently -n mcp,server,web \"npm run dev:mcp\" \"npm run dev:server\" \"npm run dev\"",
